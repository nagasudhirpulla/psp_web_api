@model IEnumerable<LabelChecksDataLayer.Models.LabelCheck>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CheckType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Num1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Num2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConsiderStartTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConsiderEndTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PspMeasurement)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CheckType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Num1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Num2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConsiderStartTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConsiderEndTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PspMeasurement.Label)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
